{
    "manifest_version": 2,
    
    "name": "Panafold Image Saver",
    "description": "Allows the user to save images that are on the page",
    "version": "1.0",
        "content_scripts" : [
    {
    "js": ["jquery-3.2.1.min.js", "content.js"],
    "matches": ["<all_urls>"]
    }],
    "permissions": [
    "contextMenus"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "background": {
        "scripts": ["background.js"]
}
}