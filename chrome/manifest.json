{
  "name": "Panafold",
  "description": "",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "background"
  ],
  
  "content_scripts" : [
  {

      "js": ["helloworld.js"],
      "matches": ["<all_urls>"]
  }],
  
  "browser_action": {
      "default_icon": "icon.png"
  },
  
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
 
  "manifest_version": 2
}

