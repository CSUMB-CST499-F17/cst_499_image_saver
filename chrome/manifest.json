{
  "name": "Panafold",
  "description": "",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  
  "content_scripts" : [
  {
      "js": ["jquery-3.2.1.min.js", "content.js"],
      "matches": ["<all_urls>"]
  }],
  
  "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "build/index.html"
  },
  
  "background": {
      "scripts": ["background.js"]
  },
 
  "manifest_version": 2
}
